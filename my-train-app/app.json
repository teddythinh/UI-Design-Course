            }
        },
        "web": {
            "favicon": ""
        },
        "plugins": [
            [
                "expo-location",
                {
                    "locationAlwaysAndWhenInUsePermission": "Allow $(my-train) to use your location.",
                    "locationWhenInUsePermission": "Allow $(my-train) to use your location.",
